<Page x:Class="MedbayTech.GraphicEditor.View.AdditionalInformationAuxiliaryRoomMedication"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedbayTech.GraphicEditor.View"
      mc:Ignorable="d" 
      d:DesignHeight="214" d:DesignWidth="274"
      Title="AdditionalInformationAuxiliaryRoomMedication">

    <Grid>
        <DataGrid Name="dataGridMedication" IsReadOnly="True" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn  Header="Medication name" Width="137" Binding="{Binding Med}" />
                <DataGridTextColumn Header="Quantity" Width="137" Binding="{Binding Quantity}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
