<Page x:Class="GraphicEditor.View.AdditionalInformationAuxiliaryRoomMedication"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GraphicEditor.View"
      mc:Ignorable="d" 
      d:DesignHeight="175" d:DesignWidth="370"
      Title="AdditionalInformationAuxiliaryRoomMedication">

    <Grid>
        <DataGrid Name="dataGridMedication" AutoGenerateColumns="False" HorizontalAlignment="Center" VerticalAlignment="Top" Width="370" Height="175" RenderTransformOrigin="1.059,-0.838">
            <DataGrid.Columns>
                <DataGridTextColumn  Header="Medication name" Width="185" Binding="{Binding Med}" />
                <DataGridTextColumn Header="Quantity" Width="185" Binding="{Binding Quantity}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
